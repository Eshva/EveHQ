        <Label x:Name="label2" Content="Corp:" Margin="21,72,0,0" VerticalAlignment="Top" Foreground="White" HorizontalAlignment="Left" Width="65"/>
        <Label x:Name="label3" Content="Isk:" Margin="21,98,0,0" VerticalAlignment="Top" Foreground="White" HorizontalAlignment="Left" Width="65"/>
        <Label x:Name="label4" Content="SkillPoints:" Margin="21,124,0,0" VerticalAlignment="Top" Foreground="White" HorizontalAlignment="Left" Width="65"/>
        <Label x:Name="PilotCorplabel" Content="Pilot Corp" Margin="86,72,0,0" VerticalAlignment="Top" Foreground="White" HorizontalAlignment="Left" Width="115"/>
        <Label x:Name="PilotIsklabel" Content="Pilot isk" Margin="86,98,0,0" VerticalAlignment="Top" Foreground="White" HorizontalAlignment="Left" Width="115"/>
        <Label x:Name="PilotSPlabel" Content="Pilot SP" Margin="86,124,0,0" VerticalAlignment="Top" Foreground="White" HorizontalAlignment="Left" Width="115"/>
    </Grid>
</UserControl>
