<Window x:Class="EveHQSharp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EveHQSharp"
        mc:Ignorable="d"
        Title="EveHQSharp Dev" Height="600" Width="800" Closed="Window_Closed" Background="#FF424242" BorderBrush="#FF424242" Foreground="#FF424242">
    <TabControl x:Name="tabControl" Margin="0,0,0,0" TabStripPlacement="Left">
        <TabItem x:Name="coreTab" Header="Core" Margin="-2,0,0,0" HorizontalAlignment="Left" Width="60" Height="50">
            <Grid x:Name="coreGrid" Initialized="coreGrid_Initialized"/>
        </TabItem>
        <TabItem x:Name="fittingTab" Header="Fitting" Margin="-2,0,0,0" HorizontalAlignment="Left" Width="60" Height="50" IsEnabled="False">
            <Grid x:Name="fittingGrid"/>
        </TabItem>
        <TabItem x:Name="pilotTab" Header="Pilots" Margin="-2,0,0,0" HorizontalAlignment="Left" Width="60" Height="50" GotFocus="pilotTab_GotFocus">
            <Grid x:Name="pilotGrid"/>
        </TabItem>
        <TabItem x:Name="industryTab" Header="Industry" Margin="-2,0,0,0" HorizontalAlignment="Left" Width="60" Height="50" IsEnabled="False">
            <Grid x:Name="industryGrid"/>
        </TabItem>
    </TabControl>
</Window>
