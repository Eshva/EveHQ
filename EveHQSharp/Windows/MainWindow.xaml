<Window x:Class="EveHQSharp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EveHQSharp"
        mc:Ignorable="d"
        Title="EveHQSharp" Closed="Window_Closed">
    <TabControl x:Name="tabControl" Margin="0,-1,0,0" SelectionChanged="tabControl_SelectionChanged" TabStripPlacement="Left">
        <TabItem x:Name="coreTab" Header="Core" Margin="-2,0,1,0">
            <Grid x:Name="coreGrid" Background="#FF001F72"/>
        </TabItem>
        <TabItem Header="Fitting" Margin="-2,0,1,0">
            <Grid Background="#FF720000"/>
        </TabItem>
        <TabItem Header="Pilots" Margin="-2,0,1,0">
            <Grid Background="#FF800074"/>
        </TabItem>
        <TabItem Header="Market" Margin="-2,0,1,0">
            <Grid Background="#FF86D300"/>
        </TabItem>
        <TabItem Header="Industry" Margin="-2,0,1,0">
            <Grid Background="#FF006A52" Margin="0,0,0,-30"/>
        </TabItem>
        <TabItem Header="TabItem" Margin="-2,0,1,0">
            <Grid Background="#FFE5E5E5"/>
        </TabItem>
        <TabItem Header="TabItem" Margin="-2,0,0,0">
            <Grid Background="#FFE5E5E5"/>
        </TabItem>
    </TabControl>
</Window>
